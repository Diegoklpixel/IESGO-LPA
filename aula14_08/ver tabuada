using System;

class Progran
{
    
    static void Main()
    {
        Console.WriteLine("Digite um número para ver a tabuada:");
        int num = Convert.ToInt32(Console.ReadLine());
       
       for (int i = 0;i <= 10; i++){
           Console.WriteLine($"{num}  X  {i} = {num * i}");
       }
    }
}
